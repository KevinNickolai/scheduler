{
	"$schema": "https://json.schemastore.org/tsconfig",
	"compilerOptions": {
		"allowJs": true,
		"esModuleInterop": true,
		"forceConsistentCasingInFileNames": true,
		"lib": [ "ES2020" ],
		"module": "CommonJS",
		"moduleResolution": "Node",
		"outDir": "build",
		"skipLibCheck": true,
		"strict": true,
		"target": "ES2020",
		"types": [ "node" ]
	},
	"display": "Node 14",
	"exclude": [
		"node_modules",
		"src",
		"build",
		"test/*",
		"test",
		"classes/d3timeImport.ts",
		"classes/d3timeImport.js"
	],
	"include": [ "./*.ts", "./commands/*.js", "classes/database/*.ts", "classes/*.ts", "events/*.ts", "classes/schedule.ts", "classes/events/scheduleEvent.ts", "commands/types.ts", "classes/events/durationEvent.ts", "classes/events/Interfaces/IDurationEvent.ts", "commands/display.ts", "commands/invite.ts", "commands/shortcut.ts" ]
}
