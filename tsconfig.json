{
	"$schema": "https://json.schemastore.org/tsconfig",
	"display": "Node 14",

	"compilerOptions": {
		"lib": [ "es2020" ],
		"module": "commonjs",
		"target": "es2020",
		"resolveJsonModule": true,
		"strict": true,
		"esModuleInterop": true,
		"skipLibCheck": true,
		"forceConsistentCasingInFileNames": true,
		"outDir": "build",
		"allowJs": true
	},
	"include": ["./*.ts", "./commands/*.js", "classes/database/*.ts", "classes/*.ts", "events/*.ts"],
	"exclude": [
		"node_modules",
		"src",
		"build",
		"test/*",
		"test"
	]
}
